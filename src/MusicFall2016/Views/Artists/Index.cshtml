@model IEnumerable<MusicFall2016.Models.Artist>
@{
    ViewData["Title"] = "Artists";
}
<h2>Artist List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Artist</th>
            <th>Genre</th>
        </tr>
    </thead>
    @foreach (var artist in Model)
    {
        <tr>
            <td>@artist.Name</td>
            <td></td>
            <td></td>
            <td>@artist.Bio</td>
        </tr>
    }
    </table>

